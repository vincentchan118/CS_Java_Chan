import java.util.Scanner;

public class AddingValuesForLoop
{
	public static void main( String[] args )
	{
		Scanner  keyboard = new Scanner(System.in);
		
		int num, t;
		t = 0;
		System.out.print("Number: ");
		num = keyboard.nextInt();
		
		for (int n = 1; n!=num; n=n+1)
		{
			System.out.print(n + " ");
			t=n+n;
		}
		System.out.print(t);
	}
}
