public class YourSchedule {
	
	public static void main(String[] args)
	{
		String a, b, c, d, e, f, g, h;
		
		a = "AP Physics 1";
		b = "Exploring Computer Science";
		c = "AP US History";
		d = "AP Calculus AB";
		e = "Corman";
		f = "Mauro";
		g = "Aguilar";
		h = "Nugyen";
		
		System.out.println("-------------------------------------");
		System.out.println("|Per 1|" + a + "|" + d + "|");
		System.out.println("|Per 2|" + b + "|" + e + "|");
		System.out.println("|Per 3|" + c + "|" + f + "|");
		System.out.println("|Per 4|" + d + "|" + g + "|");
		System.out.println("-------------------------------------");
	}
}